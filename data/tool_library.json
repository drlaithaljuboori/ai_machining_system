  "tool_selection_rules": {
    "curvature_based_selection": {
      "flat_regions": {
        "primary_tools": ["face_mill", "flat_end_mill"],
        "min_diameter": 10.0,
        "preferred_flutes": [4, 6],
        "strategy": "high_material_removal"
      },
      "high_curvature_concave": {
        "primary_tools": ["ball_end_mill"],
        "max_diameter": 3.0,
        "constraint": "tool_diameter < feature_radius * 0.8",
        "strategy": "high_precision_contouring"
      },
      "high_curvature_convex": {
        "primary_tools": ["ball_end_mill", "corner_radius_end_mill"],
        "max_diameter": 6.0,
        "strategy": "adaptive_clearing"
      },
      "medium_curvature": {
        "primary_tools": ["ball_end_mill", "corner_radius_end_mill"],
        "diameter_range": [3.0, 10.0],
        "strategy": "semi_finishing"
      },
      "low_curvature": {
        "primary_tools": ["flat_end_mill", "corner_radius_end_mill"],
        "diameter_range": [6.0, 20.0],
        "strategy": "semi_finishing"
      }
    },
    
    "operation_based_selection": {
      "roughing": {
        "preferred_tools": ["roughing_end_mill", "flat_end_mill", "face_mill"],
        "diameter_range": [10.0, 80.0],
        "depth_of_cut": "aggressive",
        "stepover": "high"
      },
      "semi_finishing": {
        "preferred_tools": ["flat_end_mill", "corner_radius_end_mill"],
        "diameter_range": [6.0, 16.0],
        "depth_of_cut": "moderate",
        "stepover": "medium"
      },
      "finishing": {
        "preferred_tools": ["ball_end_mill", "flat_end_mill"],
        "diameter_range": [1.0, 10.0],
        "depth_of_cut": "light",
        "stepover": "fine"
      }
    },
    
    "material_based_rules": {
      "aluminum": {
        "preferred_coatings": ["TiN", "uncoated"],
        "helix_angle": "high",
        "clearance_angle": "high",
        "recommended_sfm_multiplier": 1.2
      },
      "steel": {
        "preferred_coatings": ["TiAlN", "AlTiN"],
        "helix_angle": "medium",
        "clearance_angle": "medium",
        "recommended_sfm_multiplier": 1.0
      },
      "stainless_steel": {
        "preferred_coatings": ["TiAlN", "AlTiN"],
        "helix_angle": "low",
        "clearance_angle": "high",
        "recommended_sfm_multiplier": 0.8
      },
      "titanium": {
        "preferred_coatings": ["AlTiN", "TiAlN"],
        "helix_angle": "low",
        "clearance_angle": "high",
        "recommended_sfm_multiplier": 0.6
      },
      "hardened_steel": {
        "preferred_coatings": ["AlTiN", "diamond"],
        "helix_angle": "low",
        "clearance_angle": "high",
        "recommended_sfm_multiplier": 0.4
      }
    }
  },
  
